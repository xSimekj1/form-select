{
  "name": "@odyzeo/form-select",
  "version": "0.1.0",
  "description": "Odyzeo form select component with Vue",
  "main": "dist/form-select.common.js",
  "source": "src/components/FormSelect.vue",
  "browser": "dist/form-select.umd.min.js",
  "style": "dist/form-select.css",
  "less": "src/less/form-select.less",
  "private": true,
  "files": [
    "dist/*",
    "src/*",
    "package.json"
  ],
  "keywords": [
    "odyzeo",
    "form",
    "select",
    "vue"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name form-select ./src/components/FormSelect.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@odyzeo/form-item": "^2.5.0",
    "slugify": "^1.6.4",
    "vue-select": "^3.16.0",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/eslint-plugin": "^7.14.5",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "eslint": "^7.25.0",
    "eslint-plugin-vue": "^7.9.0",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "normalize.css": "^8.0.1",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "indent": 0,
      "vue/script-indent": [
        2,
        4
      ],
      "vue/html-indent": [
        2,
        4
      ],
      "vue/require-v-for-key": 1,
      "vue/html-self-closing": [
        2,
        {
          "html": {
            "normal": "never",
            "component": "never"
          }
        }
      ],
      "camelcase": 0,
      "import/extensions": [
        2,
        "ignorePackages",
        {
          "js": "never",
          "vue": "never"
        }
      ],
      "import/prefer-default-export": 0,
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ]
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "browserslist": [
    "defaults"
  ]
}
